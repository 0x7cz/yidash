        "test-ui": "vitest --ui",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix"
    },
    "dependencies": {
        "date-fns": "^3.6.0",
        "fs-extra": "^11.2.0"
    },
    "devDependencies": {
        "@antfu/eslint-config": "^2.24.1",
        "@eslint/js": "^9.8.0",
        "@vitest/ui": "^2.0.5",
        "del-cli": "^5.1.0",
        "docdash-extended-chensuiyi": "^1.1.1",
        "eslint": "9.x",
        "eslint-plugin-jsdoc": "^48.10.2",
        "jsdoc": "^4.0.3",
        "lint-staged": "^15.2.7",
        "simple-git-hooks": "^2.11.1",
        "vitest": "^2.0.5"
    },
    "simple-git-hooks": {
        "pre-commit": "pnpm lint-staged"
    },
    "lint-staged": {
        "*.{js,css,scss,less,ts,jsx,vue,html,json,md,yaml}": "eslint --fix"
    }
